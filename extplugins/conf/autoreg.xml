<configuration plugin="autoreg">
    <settings name="settings">
<!--plugin actived on/off-->
        <set name="pluginactived">on</set>
<!--number of connections for self registration (level1)-->
        <set name="noclevel1">25</set>
<!--number of connections for self level 2 -->
        <set name="noclevel2">100</set>
<!--min level required to use the noc command -->
		<set name="nocminlevel">20</set>
<!--min level required to use the autoreg command -->
		<set name="adminlevel">100</set>
        </settings>
</configuration>

